{
  "version": 1.0,
  "tests": {
    "contractTests": [
      {
        "openApiOperation": "*::/*",
        "statusSuccess": {
          "enabled": true
        }
      },
      {
        "openApiOperation": "*::/*",
        "responseTime": {
          "enabled": false,
          "maxMs": 300
        }
      },
     
      {
        "openApiOperation": "*::/*",
        "headersPresent": {
          "enabled": true
        }
      }
    ],
    "contentTests": [],
    "extendTests": [],
    "variationTests": []
  },
  "assignVariables": [],
  "overwrites": [
    {
      "openApiOperation": "POST::/v1.0/publish/*",
      "overwriteRequestPathVariables": [
        {
          "key": "pubsub",
          "value": "{{pubsub}}",
          "overwrite": true
        },
        {
          "key": "topic",
          "value": "{{topic}}",
          "overwrite": true
        }
      ],
      "overwriteRequestHeaders": [
        {
          "key": "dapr-app-id",
          "value": "{{dapr-app-id}}",
          "overwrite": true
        }
      ]
    },
    

  ],
  "globals": {
    "collectionPreRequestScripts": [],
    "keyValueReplacements": {},
    "valueReplacements": {},
    "rawReplacements": [],
    "orderOfOperations": [
      "POST::/v1.0/publis/{pubsub}/{topic}"
      
    ]
  }
  
  
}