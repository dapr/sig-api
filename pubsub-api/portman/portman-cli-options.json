{
    "local": "pubsub-api/pubsub.yaml",
    "baseUrl": "http://127.0.0.1:3500",
    "output": "pubsub-api/postman/pubsub.postman.json",
    "portmanConfigFile": "pubsub-api/portman/portman.config.pubsub.json",
    "envFile": "pubsub-api/postman/pubsub.postman_globals.json",
    "includeTests": true,
    "syncPostman": false,
    "runNewman": true,
    "newmanRunOptions": {
        "environment": "pubsub-api/postman/pubsub.postman_globals.json",
        "iteration-count": 5,
        "ignore-redirects": true,
        "insecure": true
      }
  }